{
  "name": "angular2-seed",
  "version": "1.0.0",
  "description": "A simple starter Angular2 project",
  "scripts": {
    "typings-install": "./node_modules/.bin/typings install",
    "postinstall": "npm run typings-install",
    "build": "./node_modules/.bin/webpack --inline --colors --progress --display-error-details --display-cached",
    "watch": "npm run build -- --watch",
    "start": "./node_modules/.bin/webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 3000  --content-base src"
  },
  "license": "MIT",
  "devDependencies": {
    "awesome-typescript-loader": "~0.16.2",
    "bower-webpack-plugin": "^0.1.9",
    "css-loader": "^0.23.1",
    "es6-promise": "3.0.2",
    "es6-shim": "0.35.0",
    "file-loader": "^0.8.5",
    "reflect-metadata": "0.1.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "typescript": "~1.8.9",
    "typings": "^0.7.9",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "webpack-merge": "^0.8.4"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.14",
    "core-js": "^2.2.0",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "~0.6.10"
  },
  "keywords": [
    "Angular2",
    "Angular2 Dashboard",
    "Webpack"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zsiva/angular2-webpack"
  }
}
